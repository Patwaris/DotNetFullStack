@model MVCApplication.Models.EmpInfo

@{
    ViewBag.Title = "Details";
}

@{ 
    var employeeInfo = ViewData["EmpInformation"] as MVCApplication.Models.EmpInfo;
}

<h2>Employee Info</h2>

<table>
    <tr>
        <td>
            Emp. Id :
        </td>
        <td>
            @*@Model.EmpId*@
            @employeeInfo.EmpId
        </td>
    </tr>
    <tr>
        <td>
            Name :
        </td>
        <td>
            @*@Model.Name*@
            @employeeInfo.Name
            @ViewBag.LoginUser
        </td>
    </tr>
    <tr>
        <td>
            Dept :
        </td>
        <td>
            @Model.Dept
        </td>
    </tr>
</table>

